		"jsdom": "11.12.0",
		"lint-staged": "3.5.1",
		"lodash": "^4.17.10",
		"ngrok": "2.2.9",
		"node-plop": "0.7.0",
		"node-sass": "^4.9.3",
		"null-loader": "0.1.1",
		"offline-plugin": "4.8.1",
